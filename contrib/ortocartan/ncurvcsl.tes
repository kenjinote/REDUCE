(curvature 3 '(
   (the curvature of coefficients of square terms in the symmetry eqs)
   (coordinates h K F)
   (connection (- (3 2) * K ^ 2 * h / (sinh F) ^ 2)   (5 / 2 * K)
      ((- (cosh F) + (1 2) / (cosh F)) / (sinh F))  0  0  0
      (- K ^ 3 / 4 * (sinh F) ^ 2)   0
      (3 * K ^ 3 * h * (cosh F) / 4 * (sinh F) ^ 3)   0
      ((1 / 2 * (cosh F) - (cosh F)) / (sinh F))
      (K * ((cosh F) / (sinh F)) ^ 2)
      (3 * K ^ 2 / 4 * (cosh F) * (sinh F))
      (3 * K * h / 4 * (cosh F) * (sinh F))   0
      (-2 * (sinh F) / K ^ 2 * (cosh F))
      (1 / 2 * K)   ((cosh F) / (sinh F))
                       )
   (substitutions
 ncurvature
 ((cosh F) ^ 3) = ((cosh F) * ((sinh F) ^ 2 + 1))
 ncurvature
 ((cosh F) ^ 2) = ((sinh F) ^ 2 + 1)
 ncurvature
(((cosh F) * (sinh F)) ^ -2) = (((cosh F) ^ 2 - (sinh F) ^ 2)
            / ((cosh F) * (sinh F)) ^ 2)
          )
